*{
    margin: 0;
    box-sizing: border-box;
}
.header{
    width: 100%;
    background-color: #000;
    height: 94px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
}
.header__form{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.header__form-input{
    width: 169px;
    height: 44px;
    border: 1px solid;
}
.header__form-boton{
    width: 77px;
    height: 44px;
    background: #393939;
    color: #fff;
    border: none;
}
